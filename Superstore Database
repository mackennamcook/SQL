SELECT item_name, price
FROM superstore
ORDER BY price desc

SELECT SUM (price)
FROM (superstore)

SELECT AVG (price)
FROM (superstore) 

SELECT MIN (PRICE)
FROM (superstore)

SELECT MAX (price)
FROM (superstore)

SELECT COUNT (price)
FROM (superstore)

SELECT COUNT(category)
FROM (superstore)
WHERE Category="Kitchen Supplies";
